{
  "name": "tesla-inventory",
  "description": "Retrieve real-time data from Tesla Inventory.",
  "homepage": "https://nicedoc.io/Kikobeats/tesla-inventory",
  "version": "1.2.41",
  "main": "index.js",
  "author": {
    "email": "josefrancisco.verdu@gmail.com",
    "name": "Kiko Beats",
    "url": "https://kikobeats.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kikobeats/tesla-inventory.git"
  },
  "bugs": {
    "url": "https://github.com/Kikobeats/tesla-inventory/issues"
  },
  "keywords": [
    "cpo",
    "inventory",
    "tesla",
    "tesla-api",
    "teslaapi",
    "teslamotors"
  ],
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "ava": "latest",
    "cheerio": "latest",
    "ci-publish": "latest",
    "conventional-github-releaser": "latest",
    "coveralls": "latest",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "git-dirty": "latest",
    "got": "latest",
    "husky": "latest",
    "json-future": "latest",
    "lint-staged": "latest",
    "lodash": "latest",
    "npm-check-updates": "latest",
    "nyc": "latest",
    "prettier-standard": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "standard-version": "latest"
  },
  "engines": {
    "node": ">= 10"
  },
  "files": [
    "codes.json",
    "index.js",
    "info.json",
    "inventories.js",
    "prices.json",
    "scripts"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "contributors": "(git-authors-cli && finepack && git add package.json && git commit -m 'build: contributors' --no-verify) || true",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "standard-markdown README.md && standard",
    "postrelease": "npm run release:tags && npm run release:github && ci-publish",
    "prerelease": "npm run update:check && npm run contributors",
    "pretest": "npm run lint",
    "release": "standard-version -a",
    "release:github": "conventional-github-releaser -p angular",
    "release:tags": "git push --follow-tags origin HEAD:master",
    "test": "nyc ava",
    "update:check": "ncu -- --error-level 2",
    "update:codes": "(node scripts/codes.js && git add codes.json && git commit -m 'build(update): codes' --no-verify && git push) || true",
    "update:dependencies": "ncu -u",
    "update:prices": "(node scripts/prices.js && git add prices.json && git commit -m 'build(update): prices' --no-verify && git push) || true"
  },
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": [
      "finepack"
    ],
    "*.js": [
      "prettier-standard"
    ],
    "*.md": [
      "standard-markdown"
    ]
  }
}
